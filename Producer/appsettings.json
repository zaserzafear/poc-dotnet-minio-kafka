{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "datetime",
      "FormatterOptions": {
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
      }
    }
  },
  "AllowedHosts": "*",
  "Minio": {
    "Endpoint": "localhost:9000",
    "AccessKey": "minio-user",
    "SecretKey": "minio-pass",
    "Secure": false,
    "Bucket": "upload-files"
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "upload-file"
  }
}
